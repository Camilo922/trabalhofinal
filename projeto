registo Data 
	inteiro ano
	literal mes
	inteiro dia
fimregisto

registo pessoa
	inteiro NumeroDeOrdem
	inteiro Codigo
	literal Nome
	literal genero
	inteiro NumeroDeBilhete
	inteiro Telefone
	Data Nascimento
	inteiro VerificaConta
fimregisto

registo empresa
	inteiro NumeroDeOrdem
	inteiro Codigo
	literal Nome
	inteiro NIF
	inteiro telefone
	Data Nascimento
	inteiro VerificaConta	
fimregisto

registo Conta
	inteiro Codigo
	inteiro NumeroDaConta
	real Saldo
fimregisto

    real l,valor
    inteiro NumeroProcurado,p,n,j,i,x,z,Procurados,ClientesCadastrado <- 0,ClientesCadastradoEm <- 0,ContaAberta<-0,ContaAbertaEm<-0,NumeroDaConta,NumeroDaConta2,codigo
    literal NomeProcurado,q
    pessoa Cliente[10] 
    empresa ClienteEm[10] 
    conta DonoConta[10], DonoContaEm[10]

funcao logico AberturaDeConta (pessoa k, inteiro tipodeconta)
	tipodeconta <- tipodeconta
	k.VerificaConta<-1
	DonoConta[ContaAberta].Codigo <- k.Codigo
	DonoConta[ContaAberta].NumeroDaConta <- 10000000+DonoConta[ContaAberta].Codigo
	DonoConta[ContaAberta].Saldo <- 0	
	ContaAberta <- ContaAberta+1
	retorne verdadeiro
fimfuncao

funcao logico AberturaDeContaEm (empresa k, inteiro tipodeconta)
	tipodeconta <- tipodeconta
	k.VerificaConta<-1
	DonoContaEm[ContaAbertaEm].Codigo <- k.Codigo
	DonoContaEm[ContaAbertaEm].NumeroDaConta <- 10000000+DonoContaEm[ContaAbertaEm].Codigo
	DonoContaEm[ContaAbertaEm].Saldo <- 0	
	ContaAbertaEm <- ContaAbertaEm+1
	retorne verdadeiro
fimfuncao

funcao logico DepositarDinheiro(inteiro NumeroDaConta, real valor)
	//Particular
	para i de 0 ate ContaAberta-1
		se NumeroDaConta=DonoConta[i].NumeroDaConta entao
			DonoConta[i].Saldo <- DonoConta[i].Saldo+valor
		fimse
	fimpara
	//Empresarial
	para i de 0 ate ContaAbertaEm-1
		se NumeroDaConta=DonoContaEm[i].NumeroDaConta entao
			DonoContaEm[i].Saldo <- DonoContaEm[i].Saldo+valor
		fimse
	fimpara
	//retorno
	retorne verdadeiro
fimfuncao

funcao logico LevantarDinheiro (inteiro NumeroDaConta, real valor)
	//Particular
	para i de 0 ate ContaAberta-1
		se NumeroDaConta=DonoConta[i].NumeroDaConta entao
			se valor <= DonoConta[i].Saldo entao
			  DonoConta[i].Saldo <- DonoConta[i].Saldo-valor
			fimse
		fimse
	fimpara
	//Empresarial
	para i de 0 ate ContaAbertaEm-1
		se NumeroDaConta=DonoContaEm[i].NumeroDaConta entao
			se valor <= DonoContaEm[i].Saldo entao
			  DonoContaEm[i].Saldo <- DonoContaEm[i].Saldo-valor
			fimse
		fimse
	fimpara
	//retorno
	retorne verdadeiro
fimfuncao

funcao logico TransferirDinheiro (inteiro NumeroDaContaEnviar, inteiro NumeroDaContaReceber, real valor)
	para i de 0 ate ContaAberta-1
		se NumeroDaContaEnviar=DonoConta[i].NumeroDaConta entao
			para j de 0 ate ContaAberta-1
				se NumeroDaContaReceber=DonoConta[j].NumeroDaConta entao
			     se valor <= DonoConta[i].Saldo entao
				     DonoConta[i].Saldo <- DonoConta[i].Saldo-valor				  
					 DonoConta[j].Saldo <- DonoConta[j].Saldo+valor
				 fimse
				fimse
			fimpara
			para j de 0 ate ContaAbertaEm-1
				se NumeroDaContaReceber=DonoContaEm[j].NumeroDaConta entao
			      se valor <= DonoConta[i].Saldo entao
				      DonoConta[i].Saldo <- DonoConta[i].Saldo-valor				  
					  DonoContaEm[j].Saldo <- DonoContaEm[j].Saldo+valor
				  fimse
				fimse
			fimpara
		fimse
	fimpara	
	para i de 0 ate ContaAbertaEm-1
		se NumeroDaContaEnviar=DonoContaEm[i].NumeroDaConta entao
			para j de 0 ate ContaAbertaEm-1
				se NumeroDaContaReceber=DonoContaEm[j].NumeroDaConta entao
			      se valor <= DonoContaEm[i].Saldo entao
				      DonoContaEm[i].Saldo <- DonoContaEm[i].Saldo-valor				  
					  DonoContaEm[j].Saldo <- DonoContaEm[j].Saldo+valor
				  fimse
				fimse
			fimpara
			para j de 0 ate ContaAberta-1
				se NumeroDaContaReceber=DonoConta[j].NumeroDaConta entao
			      se valor <= DonoContaEm[i].Saldo entao
				      DonoContaEm[i].Saldo <- DonoContaEm[i].Saldo-valor				  
					  DonoConta[j].Saldo <- DonoConta[j].Saldo+valor
				  fimse
				fimse
			fimpara
		fimse
	fimpara
	//retorno
	retorne verdadeiro
fimfuncao

funcao real ConsultarSaldo (inteiro NumeroDaConta)
	//Particular
	para i de 0 ate ContaAberta-1
		se NumeroDaConta=DonoConta[i].NumeroDaConta entao
			l <- DonoConta[i].Saldo
		fimse
	fimpara
	//Empresarial
	para i de 0 ate ContaAbertaEm-1
		se NumeroDaConta=DonoContaEm[i].NumeroDaConta entao
			l <- DonoContaEm[i].Saldo
		fimse
	fimpara	
	//retorno
	retorne l
fimfuncao

funcao logico CadastrarClientes(pessoa k)
    Cliente[ClientesCadastrado].VerificaConta <- 0
	Cliente[ClientesCadastrado].NumeroDeOrdem <- ClientesCadastrado+1
	Cliente[ClientesCadastrado].Codigo <- 3*ClientesCadastrado+11001
	Cliente[ClientesCadastrado].Nome <- k.Nome
	Cliente[ClientesCadastrado].NumeroDeBilhete <-k. NumeroDeBilhete
	Cliente[ClientesCadastrado].Telefone <- k.Telefone
	Cliente[ClientesCadastrado].genero <- k.genero	
	Cliente[ClientesCadastrado].Nascimento.dia <-k.Nascimento.dia
	Cliente[ClientesCadastrado].Nascimento.mes <-k.Nascimento.mes	
	Cliente[ClientesCadastrado].Nascimento.ano <-k.Nascimento.ano
	ClientesCadastrado <- ClientesCadastrado+1
	retorne verdadeiro
fimfuncao

funcao logico CadastrarClientesEm(empresa k)
	ClienteEm[ClientesCadastradoEm].VerificaConta <- 0
	ClienteEm[ClientesCadastradoEm].NumeroDeOrdem <- ClientesCadastradoEm+1
	ClienteEm[ClientesCadastradoEm].Codigo <-5*ClientesCadastradoEm+10000
	ClienteEm[ClientesCadastradoEm].Nome <- k.Nome
	ClienteEm[ClientesCadastradoEm].NIF <-k. NIF
	ClienteEm[ClientesCadastradoEm].Telefone <- k.Telefone
	ClienteEm[ClientesCadastradoEm].Nascimento.dia <- k.Nascimento.dia	
	ClienteEm[ClientesCadastradoEm].Nascimento.mes <- k.Nascimento.mes	
	ClienteEm[ClientesCadastradoEm].Nascimento.ano <- k.Nascimento.ano	
	ClientesCadastradoEm <- ClientesCadastradoEm+1
	retorne verdadeiro
fimfuncao

procedimento ListarClientesParticular()
	para n de 0 ate ClientesCadastrado-1
	    mostre "==============================================\n"		
		mostre "Nº ", Cliente[n].NumeroDeOrdem ,"\n"
		mostre "Codigo de identificacao: ",Cliente[n].Codigo,"\n"
	    mostre "Nome: ", Cliente[n].Nome,"\n"
	    mostre "Genero: ", Cliente[n].genero,"\n"
	    mostre "Numero do bilhete de identidade: ", Cliente[n].NumeroDeBilhete,"\n"
	    mostre "Numero de telefone: ",Cliente[n].Telefone,"\n"
        mostre "Data de nascimento: ",Cliente[n].Nascimento.dia," de ",Cliente[n].Nascimento.mes," de ",Cliente[n].Nascimento.ano,"\n"
	    mostre "==============================================\n"
	fimpara		
fimprocedimento	

procedimento ListarClientesEmpresariais()			 			
	para n de 0 ate ClientesCadastradoEm-1
	    mostre "==============================================\n"
		mostre "Nº ", ClienteEm[n].NumeroDeOrdem, "\n"
		mostre "Codigo de identificacao: ",ClienteEm[n].Codigo,"\n"
	    mostre "Nome da empresa: ", ClienteEm[n].Nome,"\n"
	    mostre "Numero de Identificacao Fiscal: ", ClienteEm[n].NIF,"\n"
	    mostre "Numero de telefone: ",ClienteEm[n].Telefone,"\n"
	    mostre "Data de criação da empresa: ",ClienteEm[n].Nascimento.dia," de ",ClienteEm[n].Nascimento.mes," de ",ClienteEm[n].Nascimento.ano,"\n"
	    mostre "==============================================\n"	
	fimpara					
fimprocedimento

procedimento MeusDados(pessoa k)
    para i de 0 ate ClientesCadastrado-1
    	se k.codigo = Cliente[i].Codigo entao
		    mostre "==============================================\n"		
			mostre "Nº ", Cliente[i].NumeroDeOrdem ,"\n"
			mostre "Codigo de identificacao: ",Cliente[i].Codigo,"\n"
		    mostre "Nome: ", Cliente[i].Nome,"\n"
		    mostre "Genero: ", Cliente[i].genero,"\n"
		    mostre "Numero do bilhete de identidade: ", Cliente[i].NumeroDeBilhete,"\n"
		    mostre "Numero de telefone: ",Cliente[i].Telefone,"\n"
	        mostre "Data de nascimento: ",Cliente[i].Nascimento.dia," de ",Cliente[i].Nascimento.mes," de ",Cliente[i].Nascimento.ano,"\n"
		    mostre "==============================================\n"
    	fimse
    fimpara
	para i de 0 ate ContaAberta - 1
		se k.codigo=DonoConta[i].Codigo entao
			mostre "Codigo da conta: ",DonoConta[i].Codigo,"\n"
			mostre "Numero da conta: ",DonoConta[i].NumeroDaConta,"\n"
			mostre "Saldo da conta: ",DonoConta[i].Saldo,"\n"
			mostre "===========================================\n"
			mostre "Estes sao os seus dados !\n\n"
		fimse
    fimpara
fimprocedimento

procedimento MeusDadosEm(empresa k)
    para i de 0 ate ClientesCadastradoEm-1
    	se k.codigo = ClienteEm[i].Codigo entao
		    mostre "==============================================\n"
			mostre "Nº ", ClienteEm[i].NumeroDeOrdem, "\n"
			mostre "Codigo de identificacao: ",ClienteEm[i].Codigo,"\n"
		    mostre "Nome da empresa: ", ClienteEm[i].Nome,"\n"
		    mostre "Numero de Identificacao Fiscal: ", ClienteEm[i].NIF,"\n"
		    mostre "Numero de telefone: ",ClienteEm[i].Telefone,"\n"
		    mostre "Data de criação da empresa: ",ClienteEm[i].Nascimento.dia," de ",ClienteEm[i].Nascimento.mes," de ",ClienteEm[i].Nascimento.ano,"\n"
		    mostre "==============================================\n"
    	fimse
    fimpara
	para i de 0 ate ContaAbertaEm - 1
		se k.codigo=DonoContaEm[i].Codigo entao
			mostre "Codigo da conta: ",DonoContaEm[i].Codigo,"\n"
			mostre "Numero da conta: ",DonoContaEm[i].NumeroDaConta,"\n"
			mostre "Saldo da conta: ",DonoContaEm[i].Saldo,"\n"
			mostre "===========================================\n"
			mostre "Estes sao os seus dados !\n\n"
		fimse
    fimpara 
fimprocedimento
	
inicio
	
	literal quero
	inteiro opcaoConta,tipodeconta,opcao
	pessoa t
	empresa v

    quero<-"S"
	Enquanto quero="S" faca
		Mostre"================MENU================ \n"
		Mostre" Escolha a operacao que deseja: \n"
		Mostre" [01] Cadastrar cliente \n"
		Mostre" [02] Abertura de conta \n"
		Mostre" [03] Depositar dinheiro \n"
		Mostre" [04] Levantar dinheiro \n"
		Mostre" [05] Consultar saldo \n"
		Mostre" [06] Transferir dinheiro \n"
		Mostre" [07] Atualizar dados do cliente \n"
		Mostre" [08] Listar clientes \n"
		Mostre" [09] Copia de segurança\n"
		Mostre" [10] Restaurar copia de segurança \n"
		Mostre" [11] Dados do cliente \n"
		Mostre" [12] Terminar \n"
		Leia opcao
		Escolha opcao
			caso 1:
				limpatela
				quero<-"S"
				enquanto quero="S" faca
					mostre "Escolha o tipo de conta que pretende criar: \n"
					mostre "[0] Conta particular \n"
					mostre "[1] Conta empresarial \n"
					mostre "[2] Sair \n"
					leia tipodeconta
					escolha tipodeconta
						caso 0:
						    limpatela
							enquanto quero="S" faca
							    mostre "Conta particular \n"								
								mostre "Insira o seu nome: \n"
								leia t.Nome 
								limpatela
								enquanto quero="S" faca
									mostre "Escolha o genero: \n"
									mostre " [1] Masculino \n"
									mostre " [2] Feminino \n"
									leia opcao
							        escolha opcao
							        caso 1:
							        	t.genero <-"Masculino"
							        	quero <-"N"
							        caso 2:
							        	t.genero <-"Feminino"
							        	quero <- "N"
							        defeito:
										limpatela
										mostre "Essa possibilidade não existe\n"
										quero <-"S"
									fimescolha
								fimenquanto
								limpatela
								mostre "Insira o seu numero do bilhete de identidade: \n"
								leia t.NumeroDeBilhete							
								limpatela
								quero <-"S"
								enquanto quero="S" faca
									mostre "Insira o seu número de telefone: \n"
									leia t.Telefone
									se t.Telefone <= 999999999 e t.Telefone >= 900000000 entao
										t.Telefone <- t.Telefone
										quero <-"N"
									senao
										limpatela
										mostre "O número de telefone inserido não existe !\n\n"
										quero <-"S"
									fimse
								fimenquanto
								limpatela 
															
							    quero<-"S"
								enquanto quero="S" faca
									mostre "Insira o seu dia de nascimento: "
									leia t.Nascimento.dia
									limpatela
									se t.Nascimento.dia >=01 e t.Nascimento.dia <=31 entao
										se t.Nascimento.dia >=01 e t.Nascimento.dia <=29 entao
												t.Nascimento.dia <- t.Nascimento.dia
												enquanto quero="S" faca
												mostre "Dia: ",t.Nascimento.dia,"\n"
												mostre "Escolha o seu mes:\n"
												mostre " [01] Janeiro\n"
												mostre " [02] Fevereiro\n"
